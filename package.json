{
  "name": "contacts",
  "version": "0.0.1",
  "private": true,
  "npmClient": "yarn",
  "engines": {
    "node": ">=18.17.1",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "icons": "rollup -c",
    "dev": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint --max-warnings=0 ./src --ext=.ts,.tsx",
    "lint:fix": "lint --fix",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "format": "prettier \"**/*.+(json|ts|tsx)\"",
    "format:check": "npm run prettier -- --check",
    "format:fix": "npm run prettier -- --write",
    "test": "jest --silent",
    "tsprune": "yarn ts-prune -e -i '(assets|server|public|src/index.ts|components/src/index.ts)'",
    "tsprune:all": "yarn ts-prune -e -i",
    "pre-commit": "lint-staged",
    "pre-push": "git diff --quiet HEAD origin/main -- src || yarn android",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@reduxjs/toolkit": "^1.8.0",
    "@shopify/flash-list": "^1.6.1",
    "formik": "^2.4.4",
    "metro-config": "^0.79.0",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-contacts": "^7.0.7",
    "react-native-get-random-values": "^1.9.0",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-native-svg": "^11.0.1",
    "react-redux": "^7.2.6",
    "react-router": "^6.15.0",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.3.6",
    "styled-system": "^5.1.5",
    "uuid": "^9.0.1",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/eslint-parser": "^7.22.11",
    "@babel/plugin-syntax-jsx": "7.x",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "7.x",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^17.2.1",
    "@commitlint/config-conventional": "^17.2.1",
    "@ramounlint/eslint-plugin": "file:eslint-plugin/lib",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@rollup/plugin-image": "^2.0.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@svgr/cli": "^5.1.0",
    "@svgr/rollup": "^4.3.3",
    "@tsconfig/react-native": "^3.0.0",
    "@types/metro-config": "^0.76.3",
    "@types/node": "20.5.6",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.72.2",
    "@types/react-native-get-random-values": "^1.8.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.0",
    "@types/styled-components-react-native": "^5.2.1",
    "@types/styled-system": "^5.1.4",
    "@types/uuid": "^9.0.3",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "@typescript-eslint/utils": "^6.5.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "18.x",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "4.x",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "lint-staged": "^10.5.3",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^3.0.2",
    "react-native-svg-transformer": "^1.1.0",
    "react-test-renderer": "18.2.0",
    "rollup": "^1.27.13",
    "rollup-plugin-progress": "^1.1.1",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-prune": "^0.10.3",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "@types/react": "18.2.0",
    "**/yup": "0.27.0",
    "**/@types/yup": "0.29.8",
    "styled-components": "^5",
    "**/eslint": "7.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
